{
  "pipeline": {
    "name": "parse",
    "project": {
      "name": "rag-pdf"
    }
  },
  "transform": {
    "image": "garrettgoon/rag-pdf-v0.0.1",
    "cmd": ["/bin/bash", "-C"],
    "stdin": [
      "python3 parsing.py --input /pfs/documents --output /pfs/out --chunking_strategy by_title --folder_tags True"
    ],
    "env": {
      "PYTHON_UNBUFFERED": "1"
    }
  },
  "input": {
    "pfs": {
      "repo": "documents",
      "glob": "/*"
    }
  },
  "resourceRequests": {
    "cpu": 4,
    "memory": "16Gi",
    "disk": "10Gi"
  },
  "resourceLimits": {
    "gpu": {
      "type": "nvidia.com/gpu",
      "number": 1
    }
  }
}
